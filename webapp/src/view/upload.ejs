<!DOCTYPE html>
<html lang="de">

<head>
<% include ./head %>
</head>

<body>
    
    <% include ./header %>

    <section id="upload">

        <div class="container">
            <div class="row">
                <div class="col-md-6 col-md-offset-3">
                    <div class="box">
                        <form class="form-horizontal" action="#" method="post">
                            <fieldset>
                                <legend class="text-left">Login</legend>

                                <%if (typeof loginFailed != 'undefined') { %>
                                Login failed!
                                <% } %>
                                
                                <!-- File-->
                                <div class="form-group">
                                    <label class="col-md-3 control-label" for="user">Username:</label>
                                    <div class="col-md-9">
                                        <input id="file" name="file" type="file" class="form-control">
                                    </div>
                                </div>


                                <!-- Button Action -->
                                <div class="form-group">
                                    <div class="col-md-9 col-md-offset-3">
                                        <button type="submit" class="btn btn-red">Einloggen</button>

                                    </div>
                                </div>
                            </fieldset>
                        </form>

                    </div>
                    <!-- End of box -->
                </div>
                <!-- End of coloumn -->
            </div>
            <!-- End of row -->
        </div>
        <!-- End of container -->

    </section>
    <!-- End of section login -->


 <% include ./footer %>



</body>

</html>
